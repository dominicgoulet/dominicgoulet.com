<tr>
  <td class="whitespace-nowrap py-4 pl-4 text-sm font-medium text-gray-900 sm:pl-0 w-full"><%= label.name %></td>
  <td class="relative whitespace-nowrap py-4 text-right text-sm font-medium sm:pr-0 flex gap-8">
    <%= button_to 'Delete', admin_label_path(label), method: :delete, class: 'text-red-600 hover:text-red-900' %>
    <%= link_to 'Edit', edit_admin_label_path(label), class: 'text-indigo-600 hover:text-indigo-900' %>
  </td>
</tr>
